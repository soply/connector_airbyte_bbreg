{
    "documentationUrl": "https://docsurl.com",
    "connectionSpecification": {
        "$schema": "http://json-schema.org/draft-07/schema#",
        "title": "Bronnoyregistry Connector Spec",
        "type": "object",
        "required": [
            "batch_size",
            "include_objects"
        ],
        "additionalProperties": false,
        "properties": {
            "batch_size": {
                "type": "integer",
                "description": "The number of responses per request when fetching updates (maximum: 10 000)"
            },
            "max_entries": {
                "type": "integer",
                "description": "Limit how many elements are extracted per table (used in debugging). Choose -1 for no limit."
            }
        }
    }
}